    "loader-utils": "^0.2.16",
    "node-sass": "^4.5.2",
    "sass-loader": "^4.1.1",
    "vue-loader": "^10.2.1",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "2.3.2",
    "webpack-sources": "^0.1.4",
    "ws": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "execa": "^0.6.0",
